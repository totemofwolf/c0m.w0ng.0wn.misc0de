<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Disabled</key>
	<false/>
  <key>Label</key>
  <string>com.w0ng.cron.softwareupdate.daily.plist</string>
  <key>ProgramArguments</key>
  <array>
      <string>/usr/sbin/softwareupdate</string>
      <string>-liav</string>
  </array>
  <key>KeepAlive</key>
  <false/>
  <key>RunAtLoad</key>
  <false/>
  <key>StandardOutPath</key>
  <string>/var/log/softwareupdate.daily.log</string>
  <key>StandardErrorPath</key>
  <string>/var/log/softwareupdate.error.daily.log</string>
  <key>StartCalendarInterval</key>
  <dict>
      <key>Minute</key>
      <integer>30</integer>
      <key>Hour</key>
      <integer>21</integer>
  </dict>
</dict>
</plist>
