<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Disabled</key>
	<false/>
	<key>Label</key>
	<string>com.w0ng.cron.periodic.weekly.plist</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/sbin/periodic</string>
		<string>weekly</string>
	</array>
	<key>KeepAlive</key>
	<false/>
	<key>RunAtLoad</key>
	<false/>
	<key>StandardErrorPath</key>
	<string>/var/log/periodic.weekly.log</string>
	<key>StartCalendarInterval</key>
	<dict>
		<key>Minute</key>
		<integer>10</integer>
		<key>Hour</key>
		<integer>21</integer>
		<key>Weekday</key>
		<integer>6</integer>
	</dict>
</dict>
</plist>
